        text-align: center;
    }

        .nav2 li a {
            color: #fff;
            font-size: 2.2rem;
            font-weight: bold;
        }

        .nav2 li:after {
            position: absolute;
            left: 20%;
            height: 5px;
            width: 60%;
            bottom: -100%;
            background: #fff;
            content: "";
            opacity: 0;
            transition: 0.8s;
        }

        .nav2 li:hover::after {
            opacity: 1;
        }

        .nav2 li:nth-child(1):after {
            opacity: 1;
        }

