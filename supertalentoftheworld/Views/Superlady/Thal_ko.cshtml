<!--Superlady/Thal_ko-->
<style>
    .superlady-logoimg {
        float: left;
        margin-right: 30px;
    }

    .superlady-head h2 {
        color: #fff;
        font-weight: bold;
        padding-top: 40px;
    }

    .superlady-btn {
        position: absolute;
        top: 21%;
        left: 45%;
    }

    .ui-selectmenu-menu {
        position: absolute;
        display: table;
        width: 80%;
        top: 45%;
        margin: 0 10%;
    }

    .nav2 li {
        display: table;
        width: 20%;
        float: left;
        position: relative;
        text-align: center;
    }

        .nav2 li a {
            color: #fff;
            font-size: 2.2rem;
            font-weight: bold;
        }

        .nav2 li:after {
            position: absolute;
            left: 20%;
            height: 5px;
            width: 60%;
            bottom: -100%;
            background: #fff;
            content: "";
            opacity: 0;
            transition: 0.8s;
        }

        .nav2 li:hover::after {
            opacity: 1;
        }

        .nav2 li:nth-child(4):after {
            opacity: 1;
        }

    .button {
        padding: 20px;
        cursor: pointer;
        transition: all 0.3s ease-out;
    }

    .popup {
        margin: 70px auto;
        padding: 20px;
        background: #fff;
        border-radius: 5px;
        width: 240px;
        position: fixed;
        top: 15%;
        left: 38%;
        transition: opacity 500ms;
        visibility: hidden;
        opacity: 0;
    }

        .popup:target {
            visibility: visible;
            opacity: 1;
        }

        .popup img {
            width: 45px;
            margin-right: 10px;
        }

        .popup .close {
            position: absolute;
            top: 10px;
            right: 20px;
            transition: all 200ms;
            font-size: 30px;
            font-weight: bold;
            text-decoration: none;
            color: #333;
        }

            .popup .close:hover {
                color: #333;
            }

        .popup .content {
            max-height: 20%;
            overflow: auto;
        }

    .superlady-title {
        position: relative;
        margin: 0 0 30px;
        padding: 0;
        border-bottom: 1px solid #d8d8d8;
        font-size: 24px;
        clear: both;
        word-break: keep-all;
    }

        .superlady-title span {
            display: inline-block;
            padding-bottom: 15px;
            text-transform: uppercase;
            border-bottom: 5px solid transparent;
            color: #333;
        }

        .superlady-title:after {
            position: absolute;
            left: 10%;
            transform: translateX(-50%);
            height: 5px;
            width: 20%;
            bottom: 0;
            background: #162541;
            content: "";
        }

    .superlady-schtit h4 {
        width: 25%;
        float: left;
        margin-bottom: 20px;
        letter-spacing: 1px;
    }

        .superlady-schtit h4:nth-child(2n-1) {
            color: #888;
        }

        .superlady-schtit h4:nth-child(2n) {
            color: #111;
        }

    .superlady-sch {
        letter-spacing: 1px;
        width: 25%;
        float: left;
        position: relative;
    }

        .superlady-sch:before {
            position: absolute;
            left: -3%;
            transform: translateX(-50%);
            height: 100%;
            width: 3px;
            bottom: 0;
            background: #162541;
            content: "";
        }

    .in-text {
        position: absolute;
        left: 0;
        bottom: 0;
        background-color: rgba(255, 65, 87, 0.6);
        color: white;
        font-size: 0.75em;
        padding: 5px;
        width: 100%;
    }

    .carousel-control {
        width: 4%;
    }

        .carousel-control.left, .carousel-control.right {
            margin-left: 15px;
            background-image: none;
            top: 0px;
        }

    @@media (max-width: 767px) {
        .carousel-inner .active.left {
            left: -100%;
        }

        .carousel-inner .next {
            left: 100%;
        }

        .carousel-inner .prev {
            left: -100%;
        }

        .active > div {
            display: none;
        }

            .active > div:first-child {
                display: block;
            }
    }

    @@media (min-width: 767px) and (max-width: 992px ) {
        .carousel-inner .active.left {
            left: -50%;
        }

        .carousel-inner .next {
            left: 50%;
        }

        .carousel-inner .prev {
            left: -50%;
        }

        .active > div {
            display: none;
        }

            .active > div:first-child {
                display: block;
            }

                .active > div:first-child + div {
                    display: block;
                }
    }

    @@media (min-width: 992px ) {
        .carousel-inner .active.left {
            left: -16.7%;
        }

        .carousel-inner .next {
            left: 16.7%;
        }

        .carousel-inner .prev {
            left: -16.7%;
        }
    }

    @@media (max-width: 767px) {
        .superlady-head h2 {
            font-size: 2rem;
            padding-top: 20px;
        }

        .superlady-logoimg {
            width: 20%;
            margin-right: 10px;
        }

        .superlady-btn {
            left: 70%;
            top: 30%
        }

        .superlady-img {
            width: 27px;
        }

        .popup {
            top: 24%;
            left: 43%;
            width: 180px;
        }

            .popup img {
                width: 30px;
                z-index: 999999;
            }

            .popup .close {
                top: 5px;
                right: 10px;
            }

        .ui-selectmenu-menu {
            position: relative;
            display: block;
            width: 100%;
            margin: 0px;
        }

        .nav2 {
            display: block;
            text-align: center;
            background: #fff;
        }

            .nav2 li:first-child {
                border-top: solid 1px #ebebeb;
            }

            .nav2 li:last-child {
                border-bottom: solid 1px #ebebeb;
            }

            .nav2 li {
                display: block;
                width: 100%;
                padding: 10px 0;
            }

                .nav2 li a {
                    display: block;
                    font-size: 1.4rem;
                    color: #333;
                    width: 100%;
                    height: 100%;
                }

        .superlady-title {
            font-size: 22px;
        }

        .superlady-schtit h4 {
            width: 50%;
            float: left;
            font-size: 1.4rem;
        }

        .superlady-sch {
            float: none;
            width: 80%;
            margin-top: 15px;
            font-size: 1.4rem;
        }
    }
</style>

