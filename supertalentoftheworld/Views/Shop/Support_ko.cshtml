<style>
    .col-md-3, .col-md-4 {
        margin: 20px auto;
    }

    a {
        color: #0678cb;
    }

    .dis-none {
        display: none;
    }

    .support-title {
        position: relative;
        margin: 0 0 30px;
        padding: 0;
        border-bottom: 1px solid #d8d8d8;
        font-size: 32px;
        clear: both;
    }

        .support-title span {
            display: inline-block;
            padding-bottom: 15px;
            text-transform: uppercase;
            border-bottom: 5px solid transparent;
            color: #333;
        }

        .support-title:after {
            position: absolute;
            left: 10%;
            transform: translateX(-50%);
            height: 5px;
            width: 20%;
            bottom: 0;
            background: #333;
            content: "";
        }

    .submit-top-btn {
        background: #ff4157;
        border: 1px solid #ff4157;
        display: inline-block;
        font-size: 15px;
        letter-spacing: 1px;
        margin-top: 15px;
        padding: 6px 15px;
        text-transform: uppercase;
        color: #fff
    }

        .submit-top-btn:hover {
            background: #fff;
            border-color: #fff;
            color: #ff4157;
        }

    .img-hover:hover {
        transform: scale(1.1);
    }

    .support-tri:nth-child(2n):before {
        content: '';
        width: 0;
        height: 0;
        border-top: 48.5px solid transparent;
        border-right: 49px solid #f6f6f6;
        border-bottom: 48.5px solid transparent;
        border-left: none;
        position: absolute;
        left: -49px;
        top: 42px;
    }

    .support-tri:nth-child(2n+1):before {
        content: '';
        width: 0;
        height: 0;
        border-top: 48.5px solid transparent;
        border-left: 49px solid #f6f6f6;
        border-bottom: 48.5px solid transparent;
        position: absolute;
        right: -49px;
        z-index: 100;
        top: 42px;
    }

  

    .support-color {
        padding: 10px;
        border-bottom: dashed 1px #ebebeb;
    }

        .support-color div span {
            color: #ff4157;
        }
        .ml {
            margin-left:-15px;
        }

    @@media (max-width:767px) {
        .support-img div img {
            margin: auto;
        }

        .support-color div a {
            word-break: break-all;
        }
        .support-tri:before {
            display:none;
        }
        .ml {
            margin-left:0;
        }
    }
</style>
