<style>

    .carousel-fade .carousel-inner .item {
        transition: 0.6s ease-in-out opacity;
    }

    .item,
    .active.left,
    .active.right {
        opacity: 0.5;
    }

    .active,
    .next.left,
    .prev.right {
        opacity: 1;
    }

        .next,
        .prev,
        .active.left,
        .active.right {
            left: 0;
            transform: translate3d(0, 0, 0);
        }

    .carousel-control {
        z-index: 2;
    }

    .t_price {
        margin: 1% 0;
        padding: 2% 0;
        font-size: 1.2em;
        font-weight: bold;
    }

    .t_arrow {
        position: absolute;
        bottom: 5%;
        right: -4%;
    }

    .ori {
        border: solid 0.5px #b0b0b0;
    }

    .sel {
        cursor: pointer
    }



    .product-title {
        color: #ff4157;
        position: relative;
        margin: 0 0 10px 30px;
        padding: 0 0 20px;
        clear: both;
    }

        .product-title span {
            display: inline-block;
            padding-bottom: 15px;
            text-transform: uppercase;
            border-bottom: 3px solid transparent;
            color: #333;
        }

        .product-title:after {
            position: absolute;
            left: 0;
            height: 1px;
            width: 100%;
            bottom: 0;
            background: #162541;
            content: "";
        }

    .menuover {
        position: relative;
        transition: 0.2s;
        border: solid 1px #ddd;
        padding: 0;
        margin-right: 15px;
        min-height: 454px;
    }

        .menuover:hover {
            transform: translateY(-5px);
        }


    #tab-cont, #tab-cont1 {
        transition: ease-in 0.4s;
    }

    #tab-btn, #tab-btn1 {
        padding: 0;
        border: solid 1px #ff4157;
    }

        #tab-btn ul, #tab-btn1 ul {
            width: 100%;
        }

