    }

    .item,
    .active.left,
    .active.right {
        opacity: 0.5;
    }

    .active,
    .next.left,
    .prev.right {
        opacity: 1;
    }

        .next,
        .prev,
        .active.left,
        .active.right {
            left: 0;
            transform: translate3d(0, 0, 0);
        }

    .carousel-control {
        z-index: 2;
    }

    .ori {
        border: solid 0.5px #b0b0b0;
    }

    .sel {
        cursor: pointer
    }

    #tab-cont, #tab-cont1 {
