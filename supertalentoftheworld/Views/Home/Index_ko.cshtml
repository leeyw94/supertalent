                color: #fff;
            }


    .embed-container {
        position: relative;
        padding-bottom: 56.25%;
        height: 0;
        overflow: hidden;
        max-width: 100%;
    }

        .embed-container iframe, .embed-container object, .embed-container embed {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 99%;
        }

    .gallery-image {
        padding-left: 15px;
        padding-right: 15px;
        padding-top: 15px;
        padding-bottom: 5px;
    }

    .btn-size {
        font-size: 0.75em;
        margin-bottom: 30px;
    }

    .embed-container {
        width: 100%;
        height: 100%;
    }

        .embed-container video, .embed-container object, .embed-container embed {
            top: 0;
            left: 0;
            min-width: 100%;
            min-height: 100%;
            display: block;
            padding: 0;
        }

    ul, li {
        list-style: none;
    }

