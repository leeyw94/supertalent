@helper get_string(string str, int s)
{

    try
    {
        if (str.Length > s)
        {
            <span title="@str" style="cursor:pointer"> @str.Substring(0, s) .. </span>

        }
        else
        {
            <span style="cursor:pointer">
                @str
            </span>
        }
    }
    catch { }

}



