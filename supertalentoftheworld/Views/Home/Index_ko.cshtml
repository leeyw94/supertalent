     .hr-text {
      line-height: 1em;
      position: relative;
      outline: 0;
      border: 0;
      color: #333;
      text-align: center;
      height: 1.5em;
    }
    .hr-text:before {
      content: '';
      background: linear-gradient(to right, transparent, #333, transparent);
      position: absolute;
      left: 0;
      top: 50%;
      width: 100%;
      height: 1px;
    }
    .hr-text:after {
      content: attr(data-content);
      position: relative;
      display: inline-block;
      color: #333;
      padding: 0 .5em;
      line-height: 1.5em;
      background-color: #fff;
      font-weight:bold
    }


