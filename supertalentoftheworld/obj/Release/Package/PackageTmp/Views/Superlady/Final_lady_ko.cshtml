@model IEnumerable<supertalentoftheworld.Models.Model_Data>

@{
    Layout = "/Views/Shared/_Layout.cshtml";
}
@Html.Partial("/Views/Layout/Header_ko.cshtml")
<!--Finalkor/Attend_kor-->
<style>
    .finalk-logoimg {
        float: left;
        margin-right: 30px;
    }

    .finalk-head h2 {
        color: #fff;
        font-weight: bold;
        padding-top: 40px;
    }

    .finalk-btn {
        position: absolute;
        top: 21%;
        left: 45%;
    }

    .ui-selectmenu-menu {
        position: absolute;
        display: table;
        width: 80%;
        top: 45%;
        margin: 0 10%;
    }

    .nav2 li {
        display: table;
        width: 20%;
        float: left;
        position: relative;
        text-align: center;
    }

        .nav2 li a {
            color: #fff;
            font-size: 2.2rem;
            font-weight: bold;
        }

        .nav2 li:after {
            position: absolute;
            left: 20%;
            height: 5px;
            width: 60%;
            bottom: -100%;
            background: #fff;
            content: "";
            opacity: 0;
            transition: 0.8s;
        }

        .nav2 li:hover::after {
            opacity: 1;
        }

        .nav2 li:nth-child(3):after {
            opacity: 1;
        }

    .button {
        padding: 20px;
        cursor: pointer;
        transition: all 0.3s ease-out;
    }

    .popup {
        margin: 70px auto;
        padding: 20px;
        background: #fff;
        border-radius: 5px;
        width: 240px;
        position: fixed;
        top: 15%;
        left: 38%;
        transition: opacity 500ms;
        visibility: hidden;
        opacity: 0;
    }

        .popup:target {
            visibility: visible;
            opacity: 1;
        }

        .popup img {
            width: 45px;
            margin-right: 10px;
        }

        .popup .close {
            position: absolute;
            top: 10px;
            right: 20px;
            transition: all 200ms;
            font-size: 30px;
            font-weight: bold;
            text-decoration: none;
            color: #333;
        }

            .popup .close:hover {
                color: #333;
            }

        .popup .content {
            max-height: 20%;
            overflow: auto;
        }

    .finalk-title {
        position: relative;
        margin: 0 0 30px;
        padding: 0;
        border-bottom: 1px solid #d8d8d8;
        font-size: 32px;
        clear: both;
    }

        .finalk-title span {
            display: inline-block;
            padding-bottom: 15px;
            text-transform: uppercase;
            border-bottom: 5px solid transparent;
            color: #333;
        }

        .finalk-title:after {
            position: absolute;
            left: 10%;
            transform: translateX(-50%);
            height: 5px;
            width: 20%;
            bottom: 0;
            background: #162541;
            content: "";
        }

    .finalk-schtit h4 {
        width: 25%;
        float: left;
        margin-bottom: 20px;
        letter-spacing: 1px;
    }

        .finalk-schtit h4:nth-child(2n-1) {
            color: #888;
        }

        .finalk-schtit h4:nth-child(2n) {
            color: #111;
        }

    .finalk-sch {
        letter-spacing: 1px;
        width: 25%;
        float: left;
        position: relative;
    }

        .finalk-sch:before {
            position: absolute;
            left: -3%;
            transform: translateX(-50%);
            height: 100%;
            width: 3px;
            bottom: 0;
            background: #162541;
            content: "";
        }

    @@media (max-width: 767px) {
        .finalk-head h2 {
            font-size: 2rem;
            padding-top: 20px;
        }

        .finalk-logoimg {
            width: 20%;
            margin-right: 10px;
        }

        .finalk-btn {
            left: 70%;
            top: 30%
        }

        .finalk-img {
            width: 27px;
        }

        .popup {
            top: 24%;
            left: 43%;
            width: 180px;
        }

            .popup img {
                width: 30px;
                z-index: 999999;
            }

            .popup .close {
                top: 5px;
                right: 10px;
            }

        .ui-selectmenu-menu {
            position: relative;
            display: block;
            width: 100%;
            margin: 0px;
        }

        .nav2 {
            display: block;
            text-align: center;
            background: #fff;
        }

            .nav2 li:first-child {
                border-top: solid 1px #ebebeb;
            }

            .nav2 li:last-child {
                border-bottom: solid 1px #ebebeb;
            }

            .nav2 li {
                display: block;
                width: 100%;
                padding: 10px 0;
            }

                .nav2 li a {
                    display: block;
                    font-size: 1.4rem;
                    color: #333;
                    width: 100%;
                    height: 100%;
                }

        .finalk-title {
            font-size: 26px;
        }

        .finalk-schtit h4 {
            width: 50%;
            float: left;
            font-size: 1.4rem;
        }

        .finalk-sch {
            float: none;
            width: 80%;
            margin-top: 15px;
            font-size: 1.4rem;
        }
    }
</style>
<section class="image-head-wrapper" style="background-image: url('/images/Background/Finalkor-back.jpg'); background-size: 100%; padding-top:3%">
    <div class="col-md-1">
    </div>
    <div style="margin:0 10px" class="finalk-head">
        <img src="/images/Finalkor/centerlogo.png" class="finalk-logoimg" />
        <h2>슈퍼레이디 코리아 어워즈</h2>
    </div>
    <div class="finalk-btn">
        <a href="https://www.facebook.com/SuperTalentOfficial/" style="float:left;">
            <img src="~/images/Finalkor/like.png" class="finalk-img" />
        </a>
        <a class="button" href="#popup1">
            <img src="~/images/Finalkor/share.png" class="finalk-img" />
        </a>
    </div>
    <div id="popup1" class="popup">
        <a class="close" href="#">&times;</a>
        <div class="content">
            <a href="https://www.facebook.com/SuperTalentOfficial/" target="_blank"><img src="~/images/sns1.png" class="finalk-img" /></a>
            <a href="https://www.instagram.com/misssupertalent/" target="_blank"><img src="~/images/sns2.png" class="finalk-img" /></a>
            <a href="https://www.youtube.com/channel/UCbe6_o-ULwZ_tznU0ot3X7Q" target="_blank"><img src="~/images/sns3.png" class="finalk-img" /></a>
        </div>
    </div>
</section>
<div class="ui-selectmenu-menu">
    <ul class="list-unstyled nav2">
        <li><a href="/Superlady/Index_ko">소개</a></li>
        <li><a href="/Superlady/Attend_ko">참가자격</a></li>
        <li><a href="/Superlady/Final_lady_ko">2019 수상자</a></li>
        <li><a href="/Superlady/Thal_ko">시상내역 ▪&nbsp;혜택</a></li>
        <li><a href="/Apply/Index_ko">참가신청</a></li>
    </ul>
</div>

<div class="row" style="margin:0;clear:both;">
    <div class="col-md-10 col-md-offset-1">
        <h2 class="finalk-title"><span>&nbsp;&nbsp;&nbsp;&nbsp;</span></h2><br />
        <div>
            <section class="gallery-block gallery-front">
                <div class="container">
                    <div class="row">
                        @foreach (var item in Model)
                        {
                        <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
                            <div class="gallery-image" style="margin-bottom:0">
                                <img class="img-responsive" src="@item.MainImgPath" onerror="this.src='http://supertalent.theblueeye.com/upload2/error.png'">
                                <div class="overlay">
                                    <a class="info pop img-responsive" href="/Finalists/Model_details?id=@item.id"><i class="fa fa-search" aria-hidden="true"></i></a>
                                    <p><a>View Detail</a></p>
                                </div>
                            </div>
                            <div class="text-center" style="vertical-align:central; margin-top:10px">
                                <img class="img-country" style="border:1px solid #d3d3d3" src="@item.M_country_img" onerror="this.src='http://supertalent.theblueeye.com/upload2/error.png'" />
                                <text class="text-country">@item.M_country</text>
                            </div>
                            <div>
                                <p class="text-name">@item.M_name</p>
                            </div>
                            @{if (item.M_youtube != null || item.M_insta != null || item.M_facebook != null)
                                {
                                    <div class="text-center" style="margin-bottom:20px;">
                                        @{ if (item.M_insta != null)
                                            {
                                               <a href="@item.M_insta" target="_blank"><img src="/images/sns2.png" style="width:35px"/></a>
                                            }
                                        }
                                        @{ if (item.M_facebook != null)
                                            {
                                                <img src="/images/sns1.png" style="width:35px" />
                                            }
                                        }
                                        @{ if (item.M_youtube != null)
                                            {
                                                <img src="/images/sns3.png" style="width:35px" />
                                            }
                                        }
                                    </div>
                                            }
                                            else {
                                                <div style="height:55px;">
                                               
                                                </div>
                                            } }
                        </div>
                        }
                    </div>
                </div>

            </section>
        </div>
    </div>
</div>

<style>
    .text-name {
        font-weight: bold;
        font-size: 1.2em;
        margin-bottom: 20px;
        text-align:center;
    }

    .text-country {
        color: #a8a8a8;
        font-weight: bold;
        margin-bottom: 20px;
  
        
    }

    .img-country {
        width: 40px;
        height: 25px;
     
        text-align:center;
        vertical-align:middle;
    }

    .info {
        margin-top: 150px;
    }

    .user-ul {
        display: inline-block;
        margin: 0;
        padding: 0;
        list-style: none;
        text-align: center;
    }

    .user-li {
        display: inline;
        margin: 20px;
        border: 0;
        float: left;
        font-weight: bold;
    }

    .img-responsive {
        height: 340px;
        background-size: cover;
        width: 260px;
    }
</style>
