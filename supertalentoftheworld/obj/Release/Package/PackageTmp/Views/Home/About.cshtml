<style>
    a {
        color: #244ed4;
    }

    .font-user {
        font-family: 'GT-Walsheim-Regular';
        font-size: 1.8rem;
        line-height: 2.4rem;
    }

    .hr-text {
      line-height: 1em;
      position: relative;
      outline: 0;
      border: 0;
      color: #333;
      text-align: center;
      height: 1.5em;
    }
    .hr-text:before {
      content: '';
      background: linear-gradient(to right, transparent, #333, transparent);
      position: absolute;
      left: 0;
      top: 50%;
      width: 100%;
      height: 1px;
    }
    .hr-text:after {
      content: attr(data-content);
      position: relative;
      display: inline-block;
      color: #333;
      padding: 0 .5em;
      line-height: 1.5em;
      background-color: #fff;
      font-weight:bold
    }


    .about-box {
        min-height: 300px;
        margin: 5px;
        padding: 20px 0;
        word-break: break-all;
        border-top: solid 1px #ddd;
    }

        .about-box strong {
            display: inline-block;
            padding-bottom: 10px;
            font-size:16px;
        }

    .caption {
        position: relative;
        text-align:center; 
        padding:10px 0;
    }

    .gallery-image {
        margin-bottom: 15px;
        display: block;
        height: 170px;
    }

        .gallery-image img {
            height: 60%;
        }

        .gallery-image a {
            border: solid 1px #ddd;
            width: 100%;
            height: 100%;
            padding: 10px;
            display: block;
            transition:ease 0.4s;
        }

        .gallery-image p {
            color: #333;
            font-size: 14px;
            transition:ease 0.4s;
        }
        .gallery-image:hover a {
            border:solid 1px #ff4157
        }

        .gallery-image:hover p {
            right: 0%;
            bottom: 0;
            color: #fff;
            background:#ff4157;
        }

    @@media (max-width: 767px) {
        .gallery-image {
            height: 200px;
        }
    }
</style>


